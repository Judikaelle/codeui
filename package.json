{
	"name": "codeui",
	"displayName": "codeUI",
	"publisher": "ryanraposo",
	"description": "Theme builder for Visual Studio Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "updateCustomizedElements",
				"title": "Update Customized Elements"
			},
			{
				"command": "customizeElement",
				"title": "Customize Element"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "elementTreeView",
					"title": "codeUI",
					"icon": "paintbrush.png"
				}
			]
		},
		"views": {
			"elementTreeView": [
				{
					"id": "allElementsTreeView",
					"name": "UI Elements (all)"
				}
			]
		},
		"menus": {
			"view/item/context": [
				{
					"command": "customizeElement",
					"arguments": []
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"color": "^3.1.2",
		"jsonc-parser": "^2.1.0"
	}
}
